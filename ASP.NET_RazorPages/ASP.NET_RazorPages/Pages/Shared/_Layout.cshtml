    <!-- Login and register modal id=person-icon -->
    <div class="modal modal-log-reg" id="loginRegisterModal" tabindex="-1" role="dialog"
         aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">

                    <!-- Tab links -->
                    <div>
                        <button class="btn tablink fs-4 border-0"
                                onclick="openTab(event, 'loginContent')">
                            Login
                        </button>
                        <button class="btn tablink fs-4 border-0 ms-2"
                                onclick="openTab(event, 'registerContent')">
                            Register
                        </button>
                    </div>

                    <button type="button" class="close text-muted border-0" data-dismiss="modal" aria-label="Close">
                        <i class="bi bi-x-lg" aria-hidden="true"></i>
                    </button>
                </div>
                <div class="modal-body">
                    <!-- Content for login -->
                    <div id="loginContent" class="tabcontent">
                        <form>
                            <div class="form-group">
                                <label class="form-label fw-bold" for="email">Email</label>
                                <input class="form-control" type="email" id="email">
                            </div>
                            <div class="form-group mt-3">
                                <div class="row">
                                    <div class="col">
                                        <label class="form-label fw-bold" for="psw">Password</label><br>
                                    </div>
                                    <div class="col-auto">
                                        <a asp-page="" class="form-text small">Forgot password?</a>
                                    </div>
                                </div>
                                <input class="form-control" name="loginPassword" id="psw" placeholder="Password"
                                       type="password" required="" data-msg="Please enter your password">
                            </div>
                            <div class="form-group mt-3">
                                <div class="custom-control custom-checkbox">
                                    <input class="custom-control-input" id="loginRemember" type="checkbox">
                                    <label class="custom-control-label text-muted" for="loginRemember">
                                        <span class="text-sm">Remember me for 30 days</span>
                                    </label>
                                </div>
                            </div>
                            <div class="form-group">
                                <button class="btn btn-block btn-outline-dark w-100 mt-3" type="submit">
                                    <i class="fa fa-sign-in-alt me-2"></i> Log in
                                </button>
                            </div>
                        </form>
                        <hr class="my-3 hr-text letter-spacing-2" data-content="OR">
                        <div class="text-center">
                            <button class="btn btn1" data-toggle="tooltip" title="">
                                <i class="fa-fw fa-facebook-f fab"></i>
                            </button>
                            <button class="btn btn2" data-toggle="tooltip" title="">
                                <i class="fa-fw fa-google fab"></i>
                            </button>
                        </div>
                    </div>
